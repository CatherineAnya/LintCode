class Solution:
    """
    @param J: the types of stones that are jewels
    @param S: representing the stones you have
    @return: how many of the stones you have are also jewels
    """
    def numJewelsInStones(self, J, S):
        # Write your code here
        count = 0
        for i in S:
            for j in J:
                if i == j:
                    count += 1
                    continue
        return count
